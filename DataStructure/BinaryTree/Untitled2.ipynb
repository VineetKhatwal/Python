    "def reverse(x): \n",
    "\n",
    "    Reverse = 0  \n",
    "    print(reverse(124))\n",
    "    while(x > 0):    \n",
    "        Reminder = x %10    \n",
    "        Reverse = (Reverse *10) + Reminder  \n",
    "        x = x // 10 \n",
    "    return Reverse"
   ]
